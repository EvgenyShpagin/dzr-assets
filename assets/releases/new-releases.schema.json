{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["id", "images"],
    "properties": {
      "id": { "type": "string" },
      "images": {
        "type": "array",
        "minItems": 1,
        "items": {
          "type": "object",
          "required": ["url", "height", "width"],
          "properties": {
            "url": { "type": "string", "format": "uri" },
            "height": { "type": "integer", "minimum": 1 },
            "width": { "type": "integer", "minimum": 1 }
          },
          "additionalProperties": true
        }
      }
    },
    "additionalProperties": true
  }
}
